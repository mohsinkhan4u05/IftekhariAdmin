    <nav class="top-menu" ng-class="{'hidden-top-menu': hideTopMenu}">
        <div class="menu-icon-container hidden-md-up">
            <div class="animate-menu-button left-menu-toggle">
                <div><!-- --></div>
            </div>
        </div>
        <div class="menu">
            @*<span class="hidden-sm-down">
                <span class="margin-right-10 nowrap">
                    <span class="donut donut-warning"></span>
                    Last complaint : <b am-time-ago="global.LastComplaint"></b>
                  
            </span>*@
                          

        </div>
    </nav>
    <section class="page-content" ng-view></section>
    <div class="modal fade" id="exampleModel" tabindex="-1" role="dialog" aria-labelledby="" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <h4 class="modal-title" id="myModalLabel">Change password 
                    <span ng-show="UpdatingPasswordLoader">
    <i style="margin-left: 20%" class="fa fa-refresh fa-spin fa-1x fa-fw"></i><span>updating passsword..</span>
</span></h4>
                </div>
                <div class="modal-body">
                    
                     
                    <form name="myForm">
                        <div ng-hide="showOTPscreen">
                            <label for="pw1">Enter new password <b style="color: red">*</b></label>
                            <input type="password" id="pw1" name="pw1" class="form-control" ng-model="pw1" required/>
                            <label for="pw2">Confirm new password <b style="color: red">*</b></label>
                            <input type="password" id="pw2" name="pw2" class="form-control" ng-model="pw2" required pw-check="pw1"/>
                            <div class="msg-block" ng-show="myForm.$error">
                                <span class="msg-error" ng-show="myForm.pw2.$error.pwmatch">Passwords don't match.</span>
                            </div>
                        </div>
                       

                    </form>
                    <form name="otpForm">
                        <div ng-show="showOTPscreen">
                           
                            <label for="otpEntered">Please enter 4-digit OTP sent to your registered mobile <b style="color: red">*</b></label>
                            <input type="text" id="otpEntered" name="otpEntered" class="form-control" ng-model="otpEntered" required maxlength="4"/>
                            <div class="msg-block" style="color: red" ng-show="showOTPerror">
                                {{OTPerror}}
                            </div> <br/>
                            <p>If you don't recieve your OTP in next 2 mins , you may request for new OTP by clicking on the "Re-Generate OTP" button below.</p>

                            <button ng-class="counter >0 ? 'btn btn-default' : 'btn btn-success'" ng-click="showOTP()" ng-disabled="counter >0">Re-Generate OTP ({{counter}})</button>

                        </div>
                        <div ng-show="passwordSuccess">
                            <p>Congratulation!!! Your Password has been updated successfully.</p>
                            <i class="fa fa-smile-o fa-5x" aria-hidden="true"></i>
                        </div>
                    </form>
</div>
                <div class="modal-footer">
                    <button type="button" class="btn" ng-click="close()" data-dismiss="modal">Close</button>
                    <button type="button" ng-click="showOTP()" ng-disabled="!myForm.$valid" ng-hide="hideNextButton" class="btn btn-primary">Next</button>
                    <button type="button" ng-click="updatePassword()" ng-disabled="!otpForm.$valid" class="btn btn-primary" ng-show="hideNextButton">Save</button>
                </div>
            </div>
        </div>
    </div>
    <div class="main-backdrop"><!-- --></div>
</body>
